{
  "name": "phone-number",
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "description": "Semasim library for handling phone numbers",
  "version": "0.4.0",
  "scripts": {
    "tsc": "node ./node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "tsc-w": "node ./node_modules/typescript/bin/tsc -w -p ./tsconfig.json",
    "test": "node ./dist/test/main",
    "browserify": "node ./node_modules/browserify/bin/cmd . -o ./phone_number.js",
    "declare-global": "echo const global={}; | cat - ./node_modules/intl-tel-input/build/js/utils.js ./phone_number.js > ./tmp && mv ./tmp ./phone_number.js",
    "standalone": "npm run tsc && npm run browserify && npm run declare-global"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garronej/phone-number.git"
  },
  "author": "Joseph Garrone",
  "license": "MIT",
  "dependencies": {
    "intl-tel-input": "^13.0.4"
  },
  "devDependencies": {
    "@types/node": "^10.3.4",
    "typescript": "^3.0.1",
    "browserify": "^14.4.0"
  }
}
